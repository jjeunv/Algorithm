
N = int(input()) 
arr = [] 
count = [0] * N

for i in range(N) :
    arr.append(list(map(int, input().split())))


for i in arr:
    for j in i :
        print(j, end=' ')
    print()

for i in range(N) :
    for j in range(N):
        if arr[i][0:] == arr[j][0:] :
            count[i] += 1
            print(count)